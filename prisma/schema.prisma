
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CalendarPlan {
  id          String   @id @default(uuid())
  title       String
  description String?
  date        DateTime
  color       String?  @default("#3B82F6")

  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}